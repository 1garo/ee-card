{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "removeComments": true,
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true
   },
   "exclude": [
    "node_modules"
  ],
  "include": ["./src/**/*.ts"]
}